pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello from github webhook'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('releas') {
            steps {
                echo 'releas'
            }
        }
    }
}
